# Configurações de IA (Agnóstico de Plataforma)
AI_API_TOKEN=seu_token_aqui

# Endpoints (Padrão Chutes.ai)
AI_LLM_URL=https://llm.chutes.ai/v1/chat/completions
AI_EMBEDDING_URL=https://llm.chutes.ai/v1/embeddings

# Modelos Solicitados
AI_LLM_MODEL=NousResearch/Hermes-4-405B-FP8-TEE
AI_EMBEDDING_MODEL=Qwen/Qwen3-Embedding-8B


# Configuração JWT (RS256 - Assimétrico)
# Use JWKS_URL para validação dinâmica OU JWT_PUBLIC_KEY para chave estática

# URL do endpoint JWKS para obter chaves públicas dinamicamente
# Exemplo: https://janus-idp.example.com/realms/master/protocol/openid-connect/certs
JWT_JWKS_URL=https://your-janus-idp.com/realms/master/protocol/openid-connect/certs

# OU chave pública estática (PEM formatado)
# JWT_PUBLIC_KEY=-----BEGIN PUBLIC KEY-----\nMIIBIjANBgkqhkiG9w0BAQEFAAOCAQ8AMIIBCgKCAQEA...\n-----END PUBLIC KEY-----

# Algoritmo JWT (padrão: RS256 para Janus)
JWT_ALGORITHM=RS256

# Emissor JWT (padrão: janus-idp)
# Exemplo: https://janus-idp.example.com/realms/master
JWT_ISSUER=https://your-janus-idp.com/realms/master

# Configuração RabbitMQ
# URL de conexão RabbitMQ
# Formato: amqp://usuario:senha@host:porta/
RABBITMQ_URL=amqp://guest:guest@localhost:5672/

# Nome da fila RabbitMQ para eventos brutos de sessão
RABBITMQ_QUEUE=raw_sessions

# Configuração da Aplicação
# Host da aplicação (padrão: 0.0.0.0)
APP_HOST=0.0.0.0

# Porta da aplicação (padrão: 8000)
APP_PORT=8000
